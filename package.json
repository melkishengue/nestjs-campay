{
  "name": "nestjs-campay",
  "version": "1.0.0",
  "description": "Campay integration NestJS module",
  "main": "dist/index",
  "types": "dist/index",
  "scripts": {
    "start:dev": "tsc -w",
    "build": "tsc -p tsconfig.json",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint .",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": ["campay", "cameroon payment", "mobile money"],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/melkishengue/nestjs-campay"
  },
  "bugs": "https://github.com/melkishengue/nestjs-campay",
  "author": "Melkis Hengue",
  "license": "ISC",
  "peerDependencies": {
    "@nestjs/common": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.8",
    "@types/node": "18.7.1",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "jest": "^28.1.3",
    "jest-mock-axios": "^4.7.3",
    "ts-jest": "^28.0.8",
    "ts-node": "10.9.1",
    "typescript": "~4.8.2"
  },
  "dependencies": {
    "@types/uuid": "^9.0.8",
    "axios": "^1.6.7",
    "uuid": "^9.0.1"
  }
}
