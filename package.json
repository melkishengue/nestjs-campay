{
  "name": "nestjs-campay",
  "version": "0.0.0-development",
  "description": "NestJS module for Campay. Campay is a Mobile Money solutions provider in Cameroon",
  "main": "dist/index",
  "types": "dist/index",
  "scripts": {
    "start:dev": "tsc -w",
    "build": "tsc -p tsconfig.json",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint .",
    "test": "jest --config jest.config.js ./src",
    "test:watch": "jest --config jest.config.js --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "semantic-release": "semantic-release --branches main",
    "commit": "git-cz"
  },
  "keywords": [
    "campay",
    "cameroon payment",
    "mobile money"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/melkishengue/nestjs-campay"
  },
  "bugs": "https://github.com/melkishengue/nestjs-campay",
  "author": "Melkis Hengue",
  "license": "ISC",
  "peerDependencies": {
    "@nestjs/common": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.8",
    "@types/node": "18.7.1",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "jest": "^28.1.3",
    "jest-mock-axios": "^4.7.3",
    "nock": "^13.5.4",
    "semantic-release": "^23.0.5",
    "ts-jest": "^28.0.8",
    "ts-node": "10.9.1",
    "typescript": "~4.8.2"
  },
  "dependencies": {
    "@types/uuid": "^9.0.8",
    "axios": "^1.6.8",
    "uuid": "^9.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
