{
  "name": "nestjs-campay",
  "version": "1.0.0",
  "description": "Campay integration NestJS module",
  "main": "dist/index.js",
  "scripts": {
    "start:dev": "tsc -w",
    "build": "tsc -p tsconfig.json",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": ["campay", "cameroon payment", "mobile money"],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/melkishengue/nestjs-campay"
  },
  "bugs": "https://github.com/melkishengue/nestjs-campay",
  "author": "Melkis Hengue",
  "license": "ISC",
  "peerDependencies": {
    "@nestjs/common": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/node": "18.7.1",
    "jest": "28.1.3",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "~4.8.2"
  }
}
